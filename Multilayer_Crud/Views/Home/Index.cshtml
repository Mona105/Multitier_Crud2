@model IEnumerable<CommanLayer.Models.Employees>
@{
    ViewData["Title"] = "Home Page";
}

<div class="text-center">
    <h1 class="display-4">Welcome</h1>
    <table class="table-bordered">
        <tr>
            <th>Id</th>
            <th>Name</th>
            <th>Email</th>
            <th>Gender</th>
            <th>Mobile</th>

        </tr>
        
            @foreach(var emp in Model)

            {
              <tr>
                  <td>@emp.Id</td>
                   <td>@emp.Name</td>
                    <td>@emp.Email</td>
                     <td>@emp.Gender</td>
                      <td>@emp.Mobile</td>
              </tr>
            }
       
    </table>
</div>
